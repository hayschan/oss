  <span class="pl-kos">&lt;</span><span class="pl-ent">source</span> <span class="pl-c1">type</span>="<span class="pl-s">video/mp4</span>" <span class="pl-c1">data-src</span>="<span class="pl-s">lazy.mp4</span>" /&gt;
