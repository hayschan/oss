<div class="highlight highlight-source-js"><pre><span class="pl-k">var</span> <span class="pl-s1">myLazyLoad</span> <span class="pl-c1">=</span> <span class="pl-k">new</span> <span class="pl-v">LazyLoad</span><span class="pl-kos">(</span><span class="pl-kos">{</span>
  <span class="pl-c1">container</span>: <span class="pl-smi">document</span><span class="pl-kos">.</span><span class="pl-en">querySelector</span><span class="pl-kos">(</span><span class="pl-s">".scrollingPanel"</span><span class="pl-kos">)</span>
<span class="pl-kos">}</span><span class="pl-kos">)</span><span class="pl-kos">;</span></pre></div>
